<%= render 'origems/menu' %>
<%= render 'layouts/final_menu' %>
<h1>Origens</h1>

<br>
<table cellpadding="0" cellspacing="0" border="0" class="display dataTable" >
	<thead>
		<tr>
			<th>Número</th>
			<th>Descrição</th>
		</tr>
	</thead>
	<tbody>
		<% @origems.each do |origem| %>
		<tr>
			<td><%= link_to origem.id, origem %></td>
			<td><%= origem.descricao %></td>
		</tr>
		<% end %>
	</tbody>
</table>
<br>
