<%= render 'finalidades/menu' %>
<%= render 'layouts/final_menu' %>
<h1>Finalidades</h1>

<br>
<table cellpadding="0" cellspacing="0" border="0" class="display dataTable" >
	<thead>
		<tr>
			<th>Número</th>
			<th>Descrição</th>
		</tr>
	</thead>
	<tbody>
		<% @finalidades.each do |finalidade| %>
		<tr>
			<td><%= link_to finalidade.id, finalidade %></td>
			<td><%= finalidade.descricao %></td>
		</tr>
		<% end %>
	</tbody>
</table>
<br>
