<h1>Problemas</h1>

<br>
<table cellpadding="0" cellspacing="0" border="0" class="display dataTable" >
	<thead>
		<tr>
			<th>Número</th>
			<th>Descrição</th>
		</tr>
	</thead>
	<tbody>
		<% @problemas.each do |problema| %>
		<tr>

			<td><%= link_to problema.id, problema %></td>
			<td><%= problema.descricao %></td>
		</tr>
		<% end %>
	</tbody>
</table>
<br>
